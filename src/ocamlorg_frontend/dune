(library
 (name ocamlorg_frontend)
 (libraries dream ood timedesc))

(include_subdirs unqualified)

(subdir
 layouts/
 (rule
  (targets layout.ml)
  (deps layout.eml)
  (action
   (run %{bin:dream_eml} %{deps} --workspace %{workspace_root})))
