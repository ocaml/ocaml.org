(library
 (name ocamlorg_web)
 (libraries current_docker current_web current-sesame.dream dream dream-cli
   dream-livereload omd ocamlorg ocamlorg_pipeline))

(rule
 (targets asset.ml)
 (deps
  ../../asset/main.css
  (source_tree ../../asset))
 (action
  (with-stdout-to
   %{null}
   (run %{bin:ocaml-crunch} -m plain ../../asset -o %{targets}))))

(include_subdirs unqualified)
