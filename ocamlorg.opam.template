build: [
  ["dune" "subst"] {dev}
  [
    "dune"
    "build"
    "-p"
    name
    "-j"
    jobs
    "--ignore-promoted-rules"
    "@install"
    "@runtest" {with-test}
    "@doc" {with-doc}
  ]
]
pin-depends: [
  ["dream-accept.0.1.0" "git+https://github.com/tmattio/dream-accept#main"]
  ["dream-encoding.0.1.0" "git+https://github.com/tmattio/dream-encoding#main"]
]